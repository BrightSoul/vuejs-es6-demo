{
    "exclude": ["node_modules"],
    "compilerOptions": {
        "checkJs": true,
        "baseUrl": ".",
        "target": "es2015",
        "moduleResolution": "node",
        "strict": true,
        "experimentalDecorators": true,
        "paths": {
            "components/*": ["./src/components/*"],
            "models/*": ["./src/models/*"],
            "services/*": ["./src/services/*"],
            "vue": ["./node_modules/vue/dist/vue.min.js"]
        }
    }
}